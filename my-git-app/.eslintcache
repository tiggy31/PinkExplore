ner/>\n\n    } else {\n        return (\n            <div style = {userStyle}>\n               {users.map(user => (\n                   \n                   <UserItem key = {user.id} user={user} />\n               ))}\n              \n            </div>\n        )\n        \n    }\n   \n\n} \n\n\nconst userStyle = {\n    display: 'grid',\n    gridTemplateColumns: 'repeat(3,1fr)',\n    gridGap: '1rem'\n}\n\n\nUsers.protoTypes = {\n    users: PropTypes.array.isRequired,\n    loading: PropTypes.bool.isRequired,\n}\nexport default Users\n","/Users/tigistalemu/my-git-app/src/Components/layout/Spinner.js",[],"/Users/tigistalemu/my-git-app/src/Components/users/Search.js",[],"/Users/tigistalemu/my-git-app/src/Components/layout/Alert.js",[],"/Users/tigistalemu/my-git-app/src/Components/pages/About.js",[],"/Users/tigistalemu/my-git-app/src/Components/users/User.js",["101","102","103"],"import React, { Fragment,useContext,useEffect } from 'react'\nimport Spinner from '../layout/Spinner'\nimport Repos from '../repos/Repos'\nimport PropTypes from 'prop-types'\nimport {Link} from 'react-router-dom'\nimport GithubContext from '../../context/github/githubContext'\n\nexport const User = ({match}) => {\n\n       const githubContext = useContext(GithubContext)\n       const {getuser,loading,user,repos,getUsersRepos} = githubContext\n    useEffect(() => {\n       getuser(match.params.login)\n       getUsersRepos(match.params.login)\n    },[])\n\n  //eslint-disable-next-line\n\n\n\n         const {\n             name,\n             avatar_url,\n             location,\n             bio,\n             blog,\n             login,\n             html_url,\n             company,\n             email,\n             followers,\n             following,\n            public_repos,\n            public_gists,\n            hireable} = user\n\n             \n               if (loading) {\n                   return <Spinner />\n               }\n        return (\n           <Fragment>\n               <Li